    .prod_detail1 .prod_box_txt{ padding: 10px 16px;}
    .tab2{gap: 0;}
    .tab_menu{width: calc(100% / 12 * 3);}
    .tab{gap:16px; margin-bottom: 30px;}
    .patent ul{gap: 0;}
