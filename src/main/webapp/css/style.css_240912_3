    .news_count{font-size: 1.5rem;}
    .news_top{margin-bottom: 1.5rem;}
    .hst_lt ul{gap:4rem;}
    .hst_lt li{grid-template-columns: 1fr; font-weight: 700;}
    .hst_lt li span:nth-child(2){text-indent: 12rem;}
