#bn1 {
    right: 0;
}

#bn2 {
    right: -420px;
    animation: slideInOut 8s infinite; 
    animation-delay: 4s; 
}


@keyframes slideInOut {
    0% {
        right: -420px; /* Start off-screen */
    }
    10% {
        right: 0; /* Slide in */
    }
    50% {
        right: 0; /* Stay in view */
    }
    60% {
        right: 420px; /* Slide out to the right */
    }
    100% {
        right: 420px; /* End off-screen */
    }
}




