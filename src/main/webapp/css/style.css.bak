@charset "utf-8";
/* CSS Document */

.sub_top {width:100%; height:180px; background-repeat:no-repeat; background-position:center; float:left; }
.sub_top.sub_img1 {background-image:url(../images/sub_top1.png)}
.sub_top.sub_img2 {background-image:url(../images/sub_top2.png)}
.sub_top.sub_img3 {background-image:url(../images/sub_top3.png)}
.sub_top.sub_img4 {background-image:url(../images/sub_top4.png)}

.sub_top h2 { margin-top:43px; color:#fff; font-size:37px; font-weight:500;}
.sub_top p{ margin-top:13px; color:#fff; font-size:16px; font-weight:100;}

.sub_navi{width:100%; height:50px; background:#fff; float:left; border-bottom:1px #ccc solid;}
.sub_navi ul.sub_navi_li { margin:0 auto;}
.sub_navi ul.sub_navi_li li {width:200px; height:50px; line-height:50px; display:inline-block; font-size:14px;}
.sub_navi ul.sub_navi_li li a { display:block; color:#666;}
.sub_navi ul.sub_navi_li li.on a,.sub_navi ul.sub_navi_li li a:hover{color:#2d91ff; border-bottom:1px #2d91ff solid;}


.inner {width:100%; float:left; padding:50px 0;}
.inner h3 {font-size:30px; color:#404040; font-weight:500; margin-bottom:10px;}
.inner p {font-size:15px; color:#666; font-weight:100;}


/* 회사소개 */

.intro_top {width:100%; height:400px; background:url(../images/company/company_intro.png) center no-repeat; float:left;}
.intro_top h2{ color:#fff; font-weight:500; font-size:24px; margin:140px auto 15px; line-height:50px;}
.intro_top p {color:#fff; font-weight:100; font-size:18px;}


.intro_box1 { float:left; width:100%; padding-bottom:30px; }
.intro_box2 { width:100%; height:400px; background:url(../images/company/company_img1.png) center no-repeat; background-size:cover; float:left; }
.intro_box3 { float:left; width:100%; padding:40px 0; }
.intro_box4 { float:left; width:100%; height:400px; background:#404040; }

.intro_box1 h2.intro_tit { font-size:20px; line-height:32px; color:#737373; font-weight:500; margin-top:30px; }
.intro_box1 p.intro_txt {font-size:14px; color:#808080; line-height:22px; margin-top:10px;}

#intros .box_line {background:#444; width:30px; height:4px; margin:0 auto;}
#intros h5 { float:left; width:100%; margin:10px 0;}
#intros p{ float:left; width:100%; margin:10px 0; font-size:16px; line-height:25px; color:#444; font-weight:100;}
#intros .inner p{ margin:0;}
#intros .intro_box1 .box_line { margin-top:195px; }


.intro_box4 .ceo {width:800px;  height:240px; margin:0 auto; padding:80px 100px; }
#intros p .intro_box4 p{margin-top:30px;}
.intro_box4 p {width:800px; margin:-300px auto 0; text-align:left; color:#666; font-size:19px; line-height:36px; }
.intro_box4 p span{font-size:16px; color:#888;}


.history {width:1000px; margin:60px auto;}
.history_box {width:300px; float:left; margin-right:50px;}
.history_box:last-child { margin-right:0;}
.history_tit {width:100%; height:85px; border-bottom:1px #ddd solid;}
.history_tit h4 {color:#2d91ff;}
.history_tit p {color:#333;}
.history_tit .box_line {margin:10px auto 5px; width:20px; height:3px;}
.history_tit .box_line.line1 {background:#2d91ff;}
.history_tit .box_line.line2 {background:#81bdff;}
.history_tit .box_line.line3 {background:#c0deff;}

.history ul {width:100%; float:left; margin-top:10px;}
.history li {float:left; margin:10px 20px; color:#777; font-size:15px;}
.history li .year {width: 40px; float:left; text-align:left;}
.history li .cont {float:left; margin-left:10px; text-align:left; width:210px;}


.maps { width:100%; height:700px; background:url(../images/company/map.png) center no-repeat; background-size:cover; float:left; }
.map{width:800px; height:400px; padding:150px 100px; margin:0 auto;}
.map h2 {color:#fff; font-size:32px; text-align:left;}
.map p {color:#fff; font-size:16px; text-align:left; line-height:26px; margin:20px 0 50px; font-weight:100;}
.map a {color:#fff; font-size:14px; width:140px; height:40px; line-height:42px; border:1px #fff solid; text-align:center; display:block;}



/* 새소식 */
#new { width:100%; float:left; background:#fff; }
.new{width:1000px; margin:0 auto;}
.new dl{border-top:1px #ccc solid; margin-top:30px; width:1000px;}
.new dl dt{width:940px; height:50px;font-size:14px; line-height:51px; border-bottom:1px #ddd solid; padding:0 30px;  text-align:left; float:left;} 
.new dl dt a{ text-decoration:none; color:#333; } 
.new dl dt .new_tit {width:670px; float:left; padding-left:10px; background:url(../images/comm/ico_tit.png) 0 25px no-repeat;}
.new dl dt .new_date { width:80px; float:left; margin:0 80px; color:#8c8c8c;}
.new dl dt .new_ico {width:20px; height:50px; float:left; background:url(../images/comm/list_off.png) 0 20px no-repeat;}
.new dl dt.on{border-bottom:1px #eaeaea solid;}
.new dl dt.on .new_tit {background:url(../images/comm/ico_tit_on.png) 0 25px no-repeat; color:#2d91ff;}
.new dl dt.on .new_date{color:#2d91ff;}
.new dl dt.on .new_ico {background:url(../images/comm/list_on.png) 0 20px no-repeat;}
.new dl dd{font-size:13px; line-height:18px; border-bottom:1px #ddd solid; width:920px; height:auto;  padding:30px 40px; display:none; background:#f5f5f5; float:left;}  
.new dl dd p {  width:100%; float:left; font-size:13px; line-height:22px; color:#555; text-align:left; margin:10px 0; font-weight:500;}
.new dl dd p.new_contit { width:100%; font-size:19px; color:#222; float:left; margin-bottom:20px; }
.new dl dd img { float:left; text-align:center;}

/* paging */
.paging {width:1000px; float:left; text-align:center;}
.paging ul { margin:40px auto;}
.paging li {overflow: hidden;display: inline-block;vertical-align: top; font-size:14px; font-weight:100;}
.paging ul li a { color:#808080; display:block; width:30px; height:30px; border:1px #fff solid; line-height:26px;}
.paging ul li a:hover { border:1px #efefef solid; }
.paging ul li.on a { color:#2d91ff; font-weight:500;}
.paging li.first_page_btn{  text-indent:-9999px; background: url(../images/comm/btn_first.png) 0 0 no-repeat;} 
.paging li.prev_page_btn{  text-indent:-9999px; background: url(../images/comm/btn_prev.png) 0 0 no-repeat; margin-right:5px;} 
.paging li.next_page_btn{  text-indent:-9999px; background: url(../images/comm/btn_next.png) 0 0 no-repeat; margin-left:5px;} 
.paging li.last_page_btn{  text-indent:-9999px; background: url(../images/comm/btn_end.png) 0 0 no-repeat;} 
.paging li.first_page_btn:hover{ background: url(../images/comm/btn_first_over.png) 0 0 no-repeat;} 
.paging li.prev_page_btn:hover{ background: url(../images/comm/btn_prev_over.png) 0 0 no-repeat;} 
.paging li.next_page_btn:hover{ background: url(../images/comm/btn_next_over.png) 0 0 no-repeat;} 
.paging li.last_page_btn:hover{ background: url(../images/comm/btn_end_over.png) 0 0 no-repeat;} 


/* 문의하기 */
#call {width:100%; float:left; background:#f5f5f5;}
.call{ width:1000px; margin:0 auto;}
.call table{ width:100%; float:left; border-top:1px #ddd solid;}
.call th{background:#f6f6f6; height:45px; border-bottom:1px #ddd solid; border-left:1px #ddd solid; font-size:14px; color:#333;}
.call th:first-child, .call td:first-child{border-left:0;}
.call td{background:#fff; height:45px; border-bottom:1px #ddd solid; border-left:1px #ddd solid; font-size:14px; color:#666;}

.call .call_box{width:858px; border:1px #ddd solid; background:#fff; float:left; margin-top:50px; padding:40px 70px; margin-bottom:100px;}
.call .call_box h3{ width:100%; float:left; text-align:left; font-weight:500; font-size:18px;}
.call .call_box dl {float:left; width:100%; margin:10px 0 50px;}
.call .call_box dt {float:left; width:100px; border-bottom:1px #ddd solid; padding:10px; height:30px; line-height:30px; font-size:14px; text-align:left}
.call .call_box dd{float:left; width:700px; border-bottom:1px #ddd solid; padding:10px; height:30px; margin-left:18px;}
.call .call_box dd input {width:100%; float:left; height:28px; border:1px #efefef solid; color:#666;}
.call .call_box dd input.num{width:50px;}
.call .call_box dd input.mail{width:300px;}
.call .call_box dd span   { margin:0 5px; float:left; color:#666;}
.call .call_box dd textarea {width:100%; float:left; height:78px;border:1px #efefef solid; color:#666; }
.call .call_box .main_cont{height:80px;}


/* 인재경영 */
#talent {width:100%; float:left; background:#fff;}
.talent{ width:1000px; margin:0 auto;}
.talent_list {float:left; width:100%; margin-top:20px; margin-bottom:80px;}
.talent_list li {float:left; width:260px; padding:0 20px; margin:0 16px;}
.talent_list li .tal_img {float:left; width:100%;}
.talent_list li .tal_img img { }
.talent_list li .tal_tit {float:left; width:100%; font-size:22px; color:#2d91ff; margin:15px 0 10px;}
.talent_list li .tal_txt {float:left; width:100%; font-size:14px; line-height:22px; color:#999; font-weight:500;}


#employ{width:100%; float:left; background:#f5f5f5;}
.employ{ width:1000px; margin:0 auto;}
.tab{width:100%; float:left; margin-bottom:20px;}
.tab ul{ width:324px; margin:0 auto; }
.tab ul li{ width:160px; height:40px; border:1px #ccc solid; color:#999; float:left; font-size:14px; line-height:42px; margin-left:-1px; position:relative;}
.tab ul li.on{ border:1px #2d91ff solid; color:#2d91ff; background:#fff; z-index:1000}

.employ .tab_area1, .employ .tab_area2{ display:none; }
.employ_list {float:left; width:100%; margin-top:20px; margin-bottom:80px;}
.employ_list li {float:left; width:210px; margin:0 20px;}
.employ_list li .emp_img {float:left; width:100%;}
.employ_list li .emp_img img { }
.employ_list li .emp_tit{float:left; width:100%; font-size:20px; color:#666; margin:15px 0 10px;}
.employ_list li .emp_tit font { color:#2d91ff; margin-right:5px;}
.employ_list li .emp_txt {float:left; width:100%; font-size:14px; line-height:22px; color:#999; font-weight:500;}

#welfare {width:100%; float:left; background:#fff;}
.welfare{ width:1000px; margin:0 auto;}
.wel_list {float:left; width:100%;margin-bottom:80px;}
.wel_list li {float:left; width:440px; height:100px; margin:10px 30px; text-align:left;}
.wel_list li .wel_img {float:left; width:65px ; height:100px; margin-right:15px;}
.wel_list li .wel_img img { width:65px; height:65px; margin:15px 0; }
.wel_list li .wel_tit {float:left; width:360px; font-size:18px; color:#666; }
.wel_list li .wel_txt {float:left; width:360px; font-size:13px; line-height:22px; color:#999; font-weight:100;}

/* 서비스 소개 */
.sev_img1 {width:100%; height:2540px; float:left; background:url(../images/service/sev1.jpg) 0 0 no-repeat; background-size: contain; }
.sev_img2 {width:100%; height:2540px; float:left; background:url(../images/service/sev2.jpg) 0 0 no-repeat;  background-size: contain; }
